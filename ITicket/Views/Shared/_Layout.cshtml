                    <li id="liUsuarios" role="presentation"> @Html.ActionLink("Usuarios", "Index", "Usuarios")</li>
                </ul>
                <ul class="nav navbar-nav">
                    <li id="liProdutos" role="presentation"> @Html.ActionLink("Produtos", "Index", "Produtos")</li>
                </ul>
